{
  "name": "hypermusic-api",
  "version": "3.0.0-dev.1",
  "description": "Api Streaming Music HyperMusic",
  "author": "Gabriel Henrique Finotti",
  "license": "ProprietÃ¡ria",
  "private": true,
  "main": "dist/server.js",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsup src",
    "dev": "tsx watch src/server.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "commit": "git-cz",
    "check": "npm run lint && npm run format",
    "fix": "npm run lint:fix && npm run format:fix",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GabrielFinotti/HyperMusic-API.git"
  },
  "keywords": [
    "api",
    "streaming",
    "music",
    "node",
    "typescript",
    "express",
    "sequelize",
    "pg",
    "pg-hstore",
    "ioredis",
    "jsonwebtoken",
    "multer",
    "multer-s3"
  ],
  "bugs": {
    "url": "https://github.com/GabrielFinotti/HyperMusic-API/issues"
  },
  "homepage": "https://github.com/GabrielFinotti/HyperMusic-API#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
    "bcrypt": "^6.0.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "multer-s3": "^3.0.1",
    "pg": "^8.16.2",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.2",
    "winston": "^3.17.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.29.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.13",
    "@types/multer-s3": "^3.0.3",
    "@types/pg": "^8.15.4",
    "@types/sequelize": "^4.28.20",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.29.0",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  }
}
